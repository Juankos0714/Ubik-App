#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/miguel-dev/Documents/ADSO/FormativeProject/Ubik-App/frontend/node_modules/node-gyp-build-optional-packages/node_modules:/home/miguel-dev/Documents/ADSO/FormativeProject/Ubik-App/frontend/node_modules:/home/miguel-dev/Documents/ADSO/FormativeProject/Ubik-App/node_modules:/home/miguel-dev/Documents/ADSO/FormativeProject/node_modules:/home/miguel-dev/Documents/ADSO/node_modules:/home/miguel-dev/Documents/node_modules:/home/miguel-dev/node_modules:/home/node_modules:/node_modules:/home/miguel-dev/Documents/ADSO/FormativeProject/Ubik-App/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/miguel-dev/Documents/ADSO/FormativeProject/Ubik-App/frontend/node_modules/node-gyp-build-optional-packages/node_modules:/home/miguel-dev/Documents/ADSO/FormativeProject/Ubik-App/frontend/node_modules:/home/miguel-dev/Documents/ADSO/FormativeProject/Ubik-App/node_modules:/home/miguel-dev/Documents/ADSO/FormativeProject/node_modules:/home/miguel-dev/Documents/ADSO/node_modules:/home/miguel-dev/Documents/node_modules:/home/miguel-dev/node_modules:/home/node_modules:/node_modules:/home/miguel-dev/Documents/ADSO/FormativeProject/Ubik-App/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../node-gyp-build-optional-packages/optional.js" "$@"
else
  exec node  "$basedir/../node-gyp-build-optional-packages/optional.js" "$@"
fi
